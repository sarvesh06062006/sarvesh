#include <stdio.h>
    int main()
    {
    int a=10;
    int b=20;
    int c=30;
    int d=a+b+c;
    int e=a-b-c;
    int f=a*b*c;
    int g=a/b/c;
    int h=a%b%c;
    printf("sum=%d ",d);
    printf("sub=%d ",e);
    printf("mul=%d ",f);
    printf("div=%d ",g);
    printf("mod=%d ",h);
    
    return 0;
    
    }
